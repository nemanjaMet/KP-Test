<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeScreenFragment">

    <fragment
        android:id="@+id/homeScreenFragment"
        android:name="com.example.kupujemprodajemzadatak.screens.home.HomeScreenFragment"
        android:label="HomeScreenFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeScreenFragment_to_openAdScreenFragment"
            app:destination="@id/openAdScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/openAdScreenFragment"
        android:name="com.example.kupujemprodajemzadatak.screens.openad.OpenAdScreenFragment"
        android:label="OpenAdScreenFragment"
        tools:layout="@layout/fragment_open_ad"/>

</navigation>